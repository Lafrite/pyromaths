#: puissances.py:62 puissances.py:66
msgid "Compléter par un nombre de la forme $a^n$ avec $a$ et $n$ entiers :"
msgstr ""
"Escríbelos en una sola potencia de la forma $a^n$, siendo $a$ y $n$ números "
"enteros :"

#: puissances.py:77
msgid "\\item $%s^{%s} \\times %s^{%s} = \\dotfill$"
msgstr "\\item $%s^{%s} \\cdot %s^{%s} = \\hrulefill$"

#: puissances.py:79 puissances.py:87
msgid "\\item $%s^{%s}\\times%s^{%s}="
msgstr "\\item $%s^{%s}\\cdot %s^{%s}="

#: puissances.py:80
msgid "$(%s\\times%s)^{%s}="
msgstr "$(%s\\cdot %s)^{%s}="

#: puissances.py:85
msgid "\\item $%s^{%s}\\times%s^{%s}=\\dotfill$"
msgstr "\\item $%s^{%s}\\cdot %s^{%s}=\\hrulefill$"

#: puissances.py:91
msgid "\\item $(%s^{%s})^{%s}=\\dotfill$"
msgstr "\\item $(%s^{%s})^{%s}=\\hrulefill$"

#: puissances.py:95
msgid "$%s^{%s\\times%s}="
msgstr "$%s^{%s\\cdot %s}="

#: puissances.py:100
msgid "\\item $\\dfrac{%s^{%s}}{%s^{%s}}=\\dotfill$"
msgstr "\\item $\\dfrac{%s^{%s}}{%s^{%s}}=\\hrulefill$"

#: puissances.py:148 puissances.py:152
msgid "Écrire sous la forme d'une puissance de 10 puis donner l'écriture"
msgstr "Escríbelos en una sola potencia de 10 y después calcula dicha potencia"

#: puissances.py:149 puissances.py:153
msgid " décimale de ces nombres :"
msgstr " para dar su número decimal :"

#: puissances.py:168
msgid "\\item $10^{%s} \\times 10^{%s} = \\dotfill$"
msgstr "\\item $10^{%s} \\cdot 10^{%s} = \\hrulefill$"

#: puissances.py:170
msgid "\\item $10^{%s}\\times 10^{%s}="
msgstr "\\item $10^{%s}\\cdot 10^{%s}="

#: puissances.py:178
msgid "\\item $(10^{%s})^{%s}=\\dotfill$"
msgstr "\\item $(10^{%s})^{%s}=\\hrulefill$"

#: puissances.py:181
msgid "10^{%s \\times %s}="
msgstr "10^{%s \\cdot %s}="

#: puissances.py:188
msgid "\\item $\\dfrac{10^{%s}}{10^{%s}}=\\dotfill$"
msgstr "\\item $\\dfrac{10^{%s}}{10^{%s}}=\\hrulefill$"

#: puissances.py:222 puissances.py:226
msgid "Compléter par le nombre qui convient :"
msgstr ""
"Complétalos con la potencia de 10 adecuada para que estén en notación "
"científica :"

#: puissances.py:236
msgid "\\item $%s=%s\\times\\dotfill$"
msgstr "\\item $%s=%s\\, \\cdot\\, \\hrulefill$"

#: puissances.py:238
msgid "\\item $%s=%s\\times\\mathbf{10^{%s}}$"
msgstr "\\item $%s~=~%s~\\cdot~\\underline{\\mathbf{10^{%s}}}$"

#: puissances.py:244
msgid "\\item $%s\\times\\dotfill=%s$"
msgstr "\\item $%s\\, \\cdot\\, \\hrulefill=%s$"

#: puissances.py:246
msgid "\\item $%s\\times\\mathbf{10^{%s}}=%s$"
msgstr "\\item $%s~\\cdot~\\underline{\\mathbf{10^{%s}}}~=~%s$"

#: puissances.py:261 puissances.py:264
msgid ""
"Calculer les expressions suivantes et donner l'écriture scientifique du "
"résultat."
msgstr ""
"Calcula las siguientes expresiones y escribe el resultado en notación "
"científica."

#: puissances.py:300
msgid ""
"\\cfrac{\\nombre{%s} \\times 10^{%s} \\times \\nombre{%s} \\times 10^{%s}}"
"{\\nombre{%s} \\times \\big( 10^{%s} \\big) ^%s}"
msgstr ""
"\\cfrac{\\nombre{%s} \\cdot 10^{%s} \\, \\cdot\\, \\nombre{%s} \\cdot 10^"
"{%s}}{\\nombre{%s} \\, \\cdot\\, \\big( 10^{%s} \\big) ^%s}"

#: puissances.py:307
msgid ""
"\\cfrac{\\nombre{%s} \\times \\nombre{%s}}{\\nombre{%s}} \\times \\cfrac{10^"
"{%s+(%s)}}{10^{%s \\times %s}}"
msgstr ""
"\\cfrac{\\nombre{%s} \\, \\cdot\\, \\nombre{%s}}{\\nombre{%s}} \\, \\cdot\\, "
"\\cfrac{10^{%s+(%s)}}{10^{%s \\cdot %s}}"

#: puissances.py:310
msgid ""
"\\cfrac{\\nombre{%s} \\times \\nombre{%s}}{\\nombre{%s}} \\times \\cfrac{10^"
"{%s+%s}}{10^{%s \\times %s}}"
msgstr ""
"\\cfrac{\\nombre{%s} \\, \\cdot\\, \\nombre{%s}}{\\nombre{%s}} \\, \\cdot\\, "
"\\cfrac{10^{%s+%s}}{10^{%s \\cdot %s}}"

#: puissances.py:318 puissances.py:326
msgid "\\nombre{%s} \\times 10^{%s-(%s)}"
msgstr "\\nombre{%s} \\cdot 10^{%s-(%s)}"

#: puissances.py:322 puissances.py:330
msgid "\\nombre{%s} \\times 10^{%s-%s}"
msgstr "\\nombre{%s} \\cdot 10^{%s-%s}"

#: puissances.py:338
msgid "\\nombre{%s}  \\times 10^{%s} \\times 10^{%s}"
msgstr "\\nombre{%s}  \\cdot 10^{%s} \\cdot 10^{%s}"

#: puissances.py:347
msgid "\\nombre{%s}  \\times 10^{%s}"
msgstr "\\nombre{%s}  \\cdot 10^{%s}"
