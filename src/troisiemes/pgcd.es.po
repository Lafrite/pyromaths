msgstr ""
"\\fbox{Entonces el \\textsc{M.C.D.} de \\nombre{%s} y \\nombre{%s} es %s}\n"

#: pgcd.py:102
msgid ""
"\\begin{align*}\n"
"\\cfrac{\\nombre{%s}}{\\nombre{%s}} &= \\cfrac{\\nombre{%s}\\div%s}{\\nombre"
"{%s}\\div%s}\\\\\n"
"      &= \\boxed{\\cfrac{\\nombre{%s}}{\\nombre{%s}}}\n"
"\\end{align*}"
msgstr ""
"\\begin{align*}\n"
"\\cfrac{\\nombre{%s}}{\\nombre{%s}} &= \\cfrac{\\nombre{%s} : %s}{\\nombre"
"{%s} : %s}\\\\\n"
"      &= \\boxed{\\cfrac{\\nombre{%s}}{\\nombre{%s}}}\n"
"\\end{align*}"

